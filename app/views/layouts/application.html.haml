!!!5
%html
  %head
    %title RecipeApp
    = stylesheet_link_tag 'application', media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    %header
      #flash= flash[:notice]
    %nav#sidebar
      %h3 Categories
      %ul#categories
        = render @categories
      %ul#categories
        = render @cuisines
      %h3 Ingredients
      = render @ingredients
      = link_to 'Add Recipe', new_recipe_path
    %main
      = yield


